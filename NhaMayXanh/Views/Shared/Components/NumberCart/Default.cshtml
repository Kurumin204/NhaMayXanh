    
@model List<WebFigure.ModelsView.CartItem>
@if (Model != null && Model.Count() > 0)
{
      
        <a href="#" class="minicart-btn toolbar-btn">
             <i class="ion-bag"></i>
        <span class="cart-item_count">@Model.Count()</span>
        </a>
}
else
{
      <a href="#" class="minicart-btn toolbar-btn">
             <i class="ion-bag"></i>
        <span class="cart-item_count">0</span>
        </a>
}